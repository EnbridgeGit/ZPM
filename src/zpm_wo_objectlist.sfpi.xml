    <CL_FP_INTERFACE_DATA_CHANGED classVersion="1">
     <INTERFACE_DATA href="#o1"/>
    </CL_FP_INTERFACE_DATA_CHANGED>
    <CL_FP_CODING classVersion="1">
     <INPUT_PARAMETERS/>
     <OUTPUT_PARAMETERS/>
     <INITIALIZATION>
      <FPCLINE>********************************************************************************************</FPCLINE>
      <FPCLINE>* This part of the code initialization section will convert the application server time to</FPCLINE>
      <FPCLINE>* user specific time zone and BADI will return the user date if the Time zone</FPCLINE>
      <FPCLINE>* functionality is active else will return the application server date.</FPCLINE>
      <FPCLINE>********************************************************************************************</FPCLINE>
      <FPCLINE>TABLES: tka01,</FPCLINE>
      <FPCLINE>        afvv,</FPCLINE>
      <FPCLINE>        afvvi,</FPCLINE>
      <FPCLINE>        mara,</FPCLINE>
      <FPCLINE>        ckhs,</FPCLINE>
      <FPCLINE>        t001,</FPCLINE>
      <FPCLINE>        rc29p,</FPCLINE>
      <FPCLINE>        stpo.</FPCLINE>
      <FPCLINE/>
      <FPCLINE>TYPES: BEGIN OF lty_tplnr,</FPCLINE>
      <FPCLINE>        tplnr TYPE tplnr,</FPCLINE>
      <FPCLINE>        pltxt TYPE pltxt,</FPCLINE>
      <FPCLINE>       END OF lty_tplnr.</FPCLINE>
      <FPCLINE/>
      <FPCLINE>DATA: lwa_ripw0 TYPE ripw0,</FPCLINE>
      <FPCLINE>      lv_value1 TYPE tplnr,</FPCLINE>
      <FPCLINE>      lv_value2 TYPE tplnr,</FPCLINE>
      <FPCLINE>      lv_dummy  TYPE c,</FPCLINE>
      <FPCLINE>      lwa_zone  TYPE lty_zone,</FPCLINE>
      <FPCLINE>      lit_tplnr TYPE SORTED TABLE OF lty_tplnr WITH UNIQUE KEY tplnr,</FPCLINE>
      <FPCLINE>      lwa_tplnr TYPE lty_tplnr,</FPCLINE>
      <FPCLINE>      lv_lines  TYPE sy-index.</FPCLINE>
      <FPCLINE/>
      <FPCLINE>FIELD-SYMBOLS: &lt;lfs_zone&gt; TYPE lty_zone.</FPCLINE>
      <FPCLINE/>
      <FPCLINE>CALL METHOD cl_ssf_xsf_utilities=&gt;get_bds_graphic_as_bmp</FPCLINE>
      <FPCLINE>  EXPORTING</FPCLINE>
      <FPCLINE>    p_object       = &apos;GRAPHICS&apos;</FPCLINE>
      <FPCLINE>    p_name         = &apos;ZUGLOGO&apos;</FPCLINE>
      <FPCLINE>    p_id           = &apos;BMAP&apos;</FPCLINE>
      <FPCLINE>    p_btype        = &apos;BCOL&apos;</FPCLINE>
      <FPCLINE>  RECEIVING</FPCLINE>
      <FPCLINE>    p_bmp          = xstring</FPCLINE>
      <FPCLINE>  EXCEPTIONS</FPCLINE>
      <FPCLINE>    not_found      = 1</FPCLINE>
      <FPCLINE>    internal_error = 2</FPCLINE>
      <FPCLINE>    OTHERS         = 3.</FPCLINE>
      <FPCLINE/>
      <FPCLINE>SELECT SINGLE name_text</FPCLINE>
      <FPCLINE>       FROM user_addrp</FPCLINE>
      <FPCLINE>       INTO printby_desc</FPCLINE>
      <FPCLINE>       WHERE bname = sy-uname.</FPCLINE>
      <FPCLINE/>
      <FPCLINE>IF object_list IS NOT INITIAL.</FPCLINE>
      <FPCLINE/>
      <FPCLINE>  LOOP AT object_list INTO lwa_ripw0.</FPCLINE>
      <FPCLINE>    lwa_zone-tplnr = lwa_ripw0-tplnr.</FPCLINE>
      <FPCLINE>    SPLIT lwa_zone-tplnr AT &apos;-&apos; INTO lv_value1 lv_value2 lv_dummy.</FPCLINE>
      <FPCLINE>    CONCATENATE lv_value1 lv_value2 INTO lwa_zone-zone SEPARATED BY &apos;-&apos;.</FPCLINE>
      <FPCLINE>    APPEND lwa_zone TO zones.</FPCLINE>
      <FPCLINE>    CLEAR: lwa_zone, lv_value1, lv_value2.</FPCLINE>
      <FPCLINE>  ENDLOOP.</FPCLINE>
      <FPCLINE/>
      <FPCLINE>  IF zones IS NOT INITIAL.</FPCLINE>
      <FPCLINE>    SELECT tplnr</FPCLINE>
      <FPCLINE>           pltxt</FPCLINE>
      <FPCLINE>           FROM iflotx</FPCLINE>
      <FPCLINE>           INTO TABLE lit_tplnr</FPCLINE>
      <FPCLINE>           FOR ALL ENTRIES IN zones</FPCLINE>
      <FPCLINE>           WHERE tplnr = zones-zone AND</FPCLINE>
      <FPCLINE>                 spras = sy-langu.</FPCLINE>
      <FPCLINE/>
      <FPCLINE>    IF sy-subrc EQ 0.</FPCLINE>
      <FPCLINE>      LOOP AT zones ASSIGNING &lt;lfs_zone&gt;.</FPCLINE>
      <FPCLINE>        READ TABLE lit_tplnr INTO lwa_tplnr WITH TABLE KEY tplnr = &lt;lfs_zone&gt;-zone.</FPCLINE>
      <FPCLINE>        IF sy-subrc EQ 0.</FPCLINE>
      <FPCLINE>          &lt;lfs_zone&gt;-pltxt = lwa_tplnr-pltxt.</FPCLINE>
      <FPCLINE>        ENDIF.</FPCLINE>
      <FPCLINE>      ENDLOOP.</FPCLINE>
      <FPCLINE>    ENDIF.</FPCLINE>
      <FPCLINE>  ENDIF.</FPCLINE>
      <FPCLINE>ENDIF.</FPCLINE>
      <FPCLINE/>
      <FPCLINE>IF order_header-stort IS NOT INITIAL.</FPCLINE>
      <FPCLINE>  SELECT SINGLE ktext FROM t499s</FPCLINE>
      <FPCLINE>         INTO loc_desc</FPCLINE>
      <FPCLINE>         WHERE werks = order_header-swerk AND</FPCLINE>
      <FPCLINE>               stand = order_header-stort.</FPCLINE>
      <FPCLINE>ENDIF.</FPCLINE>
     </INITIALIZATION>
     <FORMS/>
    </CL_FP_CODING>
   </cls:CL_FP_CODING>
