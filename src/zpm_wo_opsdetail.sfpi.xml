    <CL_FP_CODING classVersion="1">
     <INPUT_PARAMETERS/>
     <OUTPUT_PARAMETERS/>
     <INITIALIZATION>
      <FPCLINE>********************************************************************************************</FPCLINE>
      <FPCLINE>* This part of the code initialization section will convert the application server time to</FPCLINE>
      <FPCLINE>* user specific time zone and BADI will return the user date if the Time zone</FPCLINE>
      <FPCLINE>* functionality is active else will return the application server date.</FPCLINE>
      <FPCLINE>********************************************************************************************</FPCLINE>
      <FPCLINE>TABLES: tka01,</FPCLINE>
      <FPCLINE>        afvv,</FPCLINE>
      <FPCLINE>        afvvi,</FPCLINE>
      <FPCLINE>        mara,</FPCLINE>
      <FPCLINE>        ckhs,</FPCLINE>
      <FPCLINE>        t001,</FPCLINE>
      <FPCLINE>        rc29p,</FPCLINE>
      <FPCLINE>        stpo.</FPCLINE>
      <FPCLINE/>
      <FPCLINE>DATA: lwa_split TYPE lty_splits,</FPCLINE>
      <FPCLINE>      lwa_afvgd TYPE afvgd,</FPCLINE>
      <FPCLINE>      lwa_work_center TYPE ops_table_for_wrkcenter_dtl,</FPCLINE>
      <FPCLINE>      lit_mdoc TYPE ops_riprjt00_t_affhd_pdf,</FPCLINE>
      <FPCLINE>      lwa_mcodes TYPE lty_mcodes,</FPCLINE>
      <FPCLINE>      lwa_operation TYPE lty_operation.</FPCLINE>
      <FPCLINE/>
      <FPCLINE>FIELD-SYMBOLS: &lt;lfs_splits&gt; TYPE lty_splits,</FPCLINE>
      <FPCLINE>               &lt;lfs_imptt&gt;  TYPE lty_imptt,</FPCLINE>
      <FPCLINE>               &lt;lfs_qpcd&gt;   TYPE lty_qpcd.</FPCLINE>
      <FPCLINE/>
      <FPCLINE>IF order_header-anlzu IS NOT INITIAL.</FPCLINE>
      <FPCLINE>  SELECT SINGLE anlzux FROM t357m_t</FPCLINE>
      <FPCLINE>         INTO syscond_desc</FPCLINE>
      <FPCLINE>         WHERE spras = sy-langu AND</FPCLINE>
      <FPCLINE>               anlzu = order_header-anlzu.</FPCLINE>
      <FPCLINE>ENDIF.</FPCLINE>
      <FPCLINE/>
      <FPCLINE>CALL METHOD cl_ssf_xsf_utilities=&gt;get_bds_graphic_as_bmp</FPCLINE>
      <FPCLINE>  EXPORTING</FPCLINE>
      <FPCLINE>    p_object       = &apos;GRAPHICS&apos;</FPCLINE>
      <FPCLINE>    p_name         = &apos;ZUGLOGO&apos;</FPCLINE>
      <FPCLINE>    p_id           = &apos;BMAP&apos;</FPCLINE>
      <FPCLINE>    p_btype        = &apos;BCOL&apos;</FPCLINE>
      <FPCLINE>  RECEIVING</FPCLINE>
      <FPCLINE>    p_bmp          = xstring</FPCLINE>
      <FPCLINE>  EXCEPTIONS</FPCLINE>
      <FPCLINE>    not_found      = 1</FPCLINE>
      <FPCLINE>    internal_error = 2</FPCLINE>
      <FPCLINE>    OTHERS         = 3.</FPCLINE>
      <FPCLINE/>
      <FPCLINE>SELECT SINGLE name_text</FPCLINE>
      <FPCLINE>       FROM user_addrp</FPCLINE>
      <FPCLINE>       INTO printby_desc</FPCLINE>
      <FPCLINE>       WHERE bname = sy-uname.</FPCLINE>
      <FPCLINE/>
      <FPCLINE>IF order_header-tplnr IS NOT INITIAL.</FPCLINE>
      <FPCLINE>  CALL METHOD zcl_shoppaper_initialize=&gt;get_zone</FPCLINE>
      <FPCLINE>    EXPORTING</FPCLINE>
      <FPCLINE>      im_tplnr = order_header-tplnr</FPCLINE>
      <FPCLINE>    IMPORTING</FPCLINE>
      <FPCLINE>      ex_zonet = zone_desc.</FPCLINE>
      <FPCLINE>ENDIF.</FPCLINE>
      <FPCLINE/>
      <FPCLINE>IF order_header-stort IS NOT INITIAL.</FPCLINE>
      <FPCLINE>  SELECT SINGLE ktext FROM t499s</FPCLINE>
      <FPCLINE>         INTO loc_desc</FPCLINE>
      <FPCLINE>         WHERE werks = order_header-swerk AND</FPCLINE>
      <FPCLINE>               stand = order_header-stort.</FPCLINE>
      <FPCLINE>ENDIF.</FPCLINE>
      <FPCLINE/>
      <FPCLINE>IF order_operation IS NOT INITIAL.</FPCLINE>
      <FPCLINE>  SELECT bedid</FPCLINE>
      <FPCLINE>         bedzl</FPCLINE>
      <FPCLINE>         dauno</FPCLINE>
      <FPCLINE>         daune</FPCLINE>
      <FPCLINE>         FROM kbez</FPCLINE>
      <FPCLINE>         INTO TABLE splits</FPCLINE>
      <FPCLINE>         FOR ALL ENTRIES IN order_operation</FPCLINE>
      <FPCLINE>         WHERE bedid = order_operation-bedid AND</FPCLINE>
      <FPCLINE>               bedzl = order_operation-bedzl.</FPCLINE>
      <FPCLINE/>
      <FPCLINE>  IF sy-subrc EQ 0.</FPCLINE>
      <FPCLINE>    SORT splits BY bedid bedzl.</FPCLINE>
      <FPCLINE>  ENDIF.</FPCLINE>
      <FPCLINE/>
      <FPCLINE>  LOOP AT order_operation INTO lwa_afvgd.</FPCLINE>
      <FPCLINE>    CLEAR: lwa_operation, lwa_work_center.</FPCLINE>
      <FPCLINE>    lwa_operation-vornr = lwa_afvgd-vornr.</FPCLINE>
      <FPCLINE>    lwa_operation-bedid = lwa_afvgd-bedzl.</FPCLINE>
      <FPCLINE>    lwa_operation-bedid = lwa_afvgd-bedzl.</FPCLINE>
      <FPCLINE>    lwa_operation-packno = lwa_afvgd-packno.</FPCLINE>
      <FPCLINE>    lwa_operation-uvorn = lwa_afvgd-uvorn.</FPCLINE>
      <FPCLINE>    lwa_operation-ltxa1 = lwa_afvgd-ltxa1.</FPCLINE>
      <FPCLINE>    lwa_operation-aufpl = lwa_afvgd-aufpl.</FPCLINE>
      <FPCLINE>    lwa_operation-aplzl = lwa_afvgd-aplzl.</FPCLINE>
      <FPCLINE>    lwa_operation-dauno = lwa_afvgd-dauno.</FPCLINE>
      <FPCLINE>    lwa_operation-daune = lwa_afvgd-daune.</FPCLINE>
      <FPCLINE>    lwa_operation-arbeh = lwa_afvgd-arbei.</FPCLINE>
      <FPCLINE>    lwa_operation-fsavd = lwa_afvgd-fsavd.</FPCLINE>
      <FPCLINE>    lwa_operation-fsavz = lwa_afvgd-fsavz.</FPCLINE>
      <FPCLINE>    lwa_operation-ssavd = lwa_afvgd-ssavd.</FPCLINE>
      <FPCLINE>    lwa_operation-ssavz = lwa_afvgd-ssavz.</FPCLINE>
      <FPCLINE/>
      <FPCLINE>    READ TABLE work_center INTO lwa_work_center</FPCLINE>
      <FPCLINE>                           WITH KEY vornr = lwa_afvgd-vornr</FPCLINE>
      <FPCLINE>                                    uvorn = lwa_afvgd-uvorn</FPCLINE>
      <FPCLINE>                                    arbid = lwa_afvgd-arbid.</FPCLINE>
      <FPCLINE/>
      <FPCLINE>    lwa_operation-arbpl = lwa_work_center-arbpl.</FPCLINE>
      <FPCLINE>    lwa_operation-ktext = lwa_work_center-ktext.</FPCLINE>
      <FPCLINE>    APPEND lwa_operation TO gt_operation.</FPCLINE>
      <FPCLINE/>
      <FPCLINE/>
      <FPCLINE>*        ltxa1 TYPE afvgd-ltxa1,</FPCLINE>
      <FPCLINE>*        dauno TYPE afvgd-dauno,</FPCLINE>
      <FPCLINE>*        daune TYPE afvgd-daune,</FPCLINE>
      <FPCLINE>*        arbei TYPE afvgd-arbei,</FPCLINE>
      <FPCLINE>*        arbeh TYPE afvgd-arbeh,</FPCLINE>
      <FPCLINE>*        fsavd TYPE afvgd-fsavd,</FPCLINE>
      <FPCLINE>*        fsavz TYPE afvgd-fsavz,</FPCLINE>
      <FPCLINE>*        ssavd TYPE afvgd-ssavd,</FPCLINE>
      <FPCLINE>*        ssavz TYPE afvgd-ssavz,</FPCLINE>
      <FPCLINE>*        arbpl TYPE afvgd-arbpl,</FPCLINE>
      <FPCLINE>*        ktext TYPE ops_table_for_wrkcenter_dtl-ktext,</FPCLINE>
      <FPCLINE/>
      <FPCLINE/>
      <FPCLINE>    LOOP AT splits ASSIGNING &lt;lfs_splits&gt; WHERE bedid = lwa_afvgd-bedid AND</FPCLINE>
      <FPCLINE>                                                bedzl = lwa_afvgd-bedzl.</FPCLINE>
      <FPCLINE>      &lt;lfs_splits&gt;-arbpl = lwa_afvgd-arbpl.</FPCLINE>
      <FPCLINE>      &lt;lfs_splits&gt;-anzzl = 1.</FPCLINE>
      <FPCLINE>    ENDLOOP.</FPCLINE>
      <FPCLINE>    IF sy-subrc NE 0.</FPCLINE>
      <FPCLINE>      lwa_split-bedid = lwa_afvgd-bedid.</FPCLINE>
      <FPCLINE>      lwa_split-bedzl = lwa_afvgd-bedzl.</FPCLINE>
      <FPCLINE>      lwa_split-dauno = lwa_afvgd-dauno.</FPCLINE>
      <FPCLINE>      lwa_split-daune = lwa_afvgd-daune.</FPCLINE>
      <FPCLINE>      lwa_split-arbpl = lwa_afvgd-arbpl.</FPCLINE>
      <FPCLINE>      lwa_split-anzzl = lwa_afvgd-anzzl.</FPCLINE>
      <FPCLINE>      APPEND lwa_split TO splits.</FPCLINE>
      <FPCLINE>      CLEAR lwa_split.</FPCLINE>
      <FPCLINE>    ENDIF.</FPCLINE>
      <FPCLINE>  ENDLOOP.</FPCLINE>
      <FPCLINE>ENDIF.</FPCLINE>
      <FPCLINE>lit_mdoc = prt_order.</FPCLINE>
      <FPCLINE>DELETE lit_mdoc WHERE fhmar NE &apos;P&apos;.</FPCLINE>
      <FPCLINE>IF lit_mdoc IS NOT INITIAL.</FPCLINE>
      <FPCLINE>  SELECT point</FPCLINE>
      <FPCLINE>         codct</FPCLINE>
      <FPCLINE>         codgr</FPCLINE>
      <FPCLINE>         FROM imptt</FPCLINE>
      <FPCLINE>         INTO TABLE gt_imptt</FPCLINE>
      <FPCLINE>         FOR ALL ENTRIES IN lit_mdoc WHERE point = lit_mdoc-eqpnt.</FPCLINE>
      <FPCLINE/>
      <FPCLINE>  IF gt_imptt IS NOT INITIAL.</FPCLINE>
      <FPCLINE>    SELECT katalogart</FPCLINE>
      <FPCLINE>           codegruppe</FPCLINE>
      <FPCLINE>           code</FPCLINE>
      <FPCLINE>           FROM qpcd</FPCLINE>
      <FPCLINE>           INTO TABLE gt_qpcd</FPCLINE>
      <FPCLINE>           FOR ALL ENTRIES IN gt_imptt</FPCLINE>
      <FPCLINE>           WHERE katalogart = gt_imptt-codct AND</FPCLINE>
      <FPCLINE>                 codegruppe = gt_imptt-codgr.</FPCLINE>
      <FPCLINE/>
      <FPCLINE>    IF sy-subrc EQ 0.</FPCLINE>
