     <INTERFACE_DATA href="#o1"/>
    </CL_FP_INTERFACE_DATA_CHANGED>
    <CL_FP_CODING classVersion="1">
     <INPUT_PARAMETERS/>
     <OUTPUT_PARAMETERS/>
     <INITIALIZATION>
      <FPCLINE>********************************************************************************************</FPCLINE>
      <FPCLINE>* This part of the code initialization section will convert the application server time to</FPCLINE>
      <FPCLINE>* user specific time zone and BADI will return the user date if the Time zone</FPCLINE>
      <FPCLINE>* functionality is active else will return the application server date.</FPCLINE>
      <FPCLINE>********************************************************************************************</FPCLINE>
      <FPCLINE>TABLES: tka01,</FPCLINE>
      <FPCLINE>        afvv,</FPCLINE>
      <FPCLINE>        afvvi,</FPCLINE>
      <FPCLINE>        mara,</FPCLINE>
      <FPCLINE>        ckhs,</FPCLINE>
      <FPCLINE>        t001,</FPCLINE>
      <FPCLINE>        rc29p,</FPCLINE>
      <FPCLINE>        stpo.</FPCLINE>
      <FPCLINE/>
      <FPCLINE>TYPES: BEGIN OF ty_imrg,</FPCLINE>
      <FPCLINE>        mdocm TYPE imrg-mdocm,</FPCLINE>
      <FPCLINE>        point TYPE imrg-point,</FPCLINE>
      <FPCLINE>        idate TYPE imrg-idate,</FPCLINE>
      <FPCLINE>        itime TYPE imrg-itime,</FPCLINE>
      <FPCLINE>        recdv TYPE imrg-recdv,</FPCLINE>
      <FPCLINE>        vlcod TYPE imrg-vlcod,</FPCLINE>
      <FPCLINE>       END OF ty_imrg,</FPCLINE>
      <FPCLINE/>
      <FPCLINE>       BEGIN OF ty_cabn,</FPCLINE>
      <FPCLINE>        atinn TYPE cabn-atinn,</FPCLINE>
      <FPCLINE>        msehi TYPE msehi,</FPCLINE>
      <FPCLINE>       END OF ty_cabn,</FPCLINE>
      <FPCLINE/>
      <FPCLINE>       BEGIN OF ty_qpct,</FPCLINE>
      <FPCLINE>         katalogart TYPE qpct-katalogart,</FPCLINE>
      <FPCLINE>         codegruppe TYPE qpct-codegruppe,</FPCLINE>
      <FPCLINE>         code       TYPE qpct-code,</FPCLINE>
      <FPCLINE>         kurztext   TYPE qpct-kurztext,</FPCLINE>
      <FPCLINE>       END OF ty_qpct.</FPCLINE>
      <FPCLINE/>
      <FPCLINE>FIELD-SYMBOLS: &lt;fs_imptt&gt; LIKE LINE OF gt_imptt,</FPCLINE>
      <FPCLINE>               &lt;fs_imrg&gt;  TYPE ty_imrg,</FPCLINE>
      <FPCLINE>               &lt;fs_cabn&gt;  TYPE ty_cabn,</FPCLINE>
      <FPCLINE>               &lt;fs_qpct&gt;  TYPE ty_qpct.</FPCLINE>
      <FPCLINE/>
      <FPCLINE>DATA: lt_prt_order TYPE ops_riprjt00_t_affhd_pdf,</FPCLINE>
      <FPCLINE>      lt_imptt TYPE zpmt_mdoc_prt,</FPCLINE>
      <FPCLINE>      lv_num   TYPE p DECIMALS 2,</FPCLINE>
      <FPCLINE>      ls_valcod LIKE LINE OF gt_valcodes,</FPCLINE>
      <FPCLINE>      lt_imrg  TYPE STANDARD TABLE OF ty_imrg,</FPCLINE>
      <FPCLINE>      lt_cabn  TYPE STANDARD TABLE OF ty_cabn,</FPCLINE>
      <FPCLINE>      lt_qpct  TYPE STANDARD TABLE OF ty_qpct.</FPCLINE>
      <FPCLINE/>
      <FPCLINE>lt_prt_order = prt_order[].</FPCLINE>
      <FPCLINE>SORT lt_prt_order BY eqpnt.</FPCLINE>
      <FPCLINE>DELETE lt_prt_order WHERE eqpnt IS INITIAL.</FPCLINE>
      <FPCLINE/>
      <FPCLINE>IF lt_prt_order IS NOT INITIAL.</FPCLINE>
      <FPCLINE>  SELECT point</FPCLINE>
      <FPCLINE>         atinn</FPCLINE>
      <FPCLINE>         desir AS desir_n</FPCLINE>
      <FPCLINE>         mrmin AS mrmin_n</FPCLINE>
      <FPCLINE>         mrmax AS mrmax_n</FPCLINE>
      <FPCLINE>         codct</FPCLINE>
      <FPCLINE>         codgr</FPCLINE>
      <FPCLINE>         FROM imptt</FPCLINE>
      <FPCLINE>         INTO TABLE gt_imptt</FPCLINE>
      <FPCLINE>         FOR ALL ENTRIES IN lt_prt_order</FPCLINE>
      <FPCLINE>         WHERE point   = lt_prt_order-eqpnt.</FPCLINE>
      <FPCLINE/>
      <FPCLINE>  IF sy-subrc EQ 0.</FPCLINE>
      <FPCLINE>    SORT gt_imptt BY point.</FPCLINE>
      <FPCLINE/>
      <FPCLINE>    lt_imptt = gt_imptt[].</FPCLINE>
      <FPCLINE>    SORT lt_imptt BY codct codgr.</FPCLINE>
      <FPCLINE>    DELETE ADJACENT DUPLICATES FROM lt_imptt COMPARING codct codgr.</FPCLINE>
      <FPCLINE/>
      <FPCLINE>    SELECT katalogart</FPCLINE>
      <FPCLINE>           codegruppe</FPCLINE>
      <FPCLINE>           code</FPCLINE>
      <FPCLINE>           kurztext</FPCLINE>
      <FPCLINE>           FROM qpct</FPCLINE>
      <FPCLINE>           INTO TABLE lt_qpct</FPCLINE>
      <FPCLINE>           FOR ALL ENTRIES IN lt_imptt</FPCLINE>
      <FPCLINE>           WHERE katalogart = lt_imptt-codct AND</FPCLINE>
      <FPCLINE>                 codegruppe = lt_imptt-codgr AND</FPCLINE>
      <FPCLINE>                 sprache    = sy-langu.</FPCLINE>
      <FPCLINE/>
      <FPCLINE>    IF sy-subrc EQ 0.</FPCLINE>
      <FPCLINE>      SORT lt_qpct BY katalogart codegruppe.</FPCLINE>
      <FPCLINE>    ENDIF.</FPCLINE>
      <FPCLINE/>
      <FPCLINE>    lt_imptt = gt_imptt.</FPCLINE>
      <FPCLINE>    SORT lt_imptt BY atinn.</FPCLINE>
      <FPCLINE>    DELETE ADJACENT DUPLICATES FROM lt_imptt COMPARING atinn.</FPCLINE>
      <FPCLINE>    DELETE lt_imptt WHERE atinn IS INITIAL.</FPCLINE>
      <FPCLINE/>
      <FPCLINE>    IF lt_imptt IS NOT INITIAL.</FPCLINE>
      <FPCLINE>      SELECT atinn</FPCLINE>
      <FPCLINE>             msehi</FPCLINE>
      <FPCLINE>             FROM cabn</FPCLINE>
      <FPCLINE>             INTO TABLE lt_cabn</FPCLINE>
      <FPCLINE>             FOR ALL ENTRIES IN lt_imptt</FPCLINE>
      <FPCLINE>             WHERE atinn = lt_imptt-atinn.</FPCLINE>
      <FPCLINE/>
      <FPCLINE>      IF sy-subrc EQ 0.</FPCLINE>
      <FPCLINE>        SORT lt_cabn BY atinn.</FPCLINE>
      <FPCLINE>      ENDIF.</FPCLINE>
      <FPCLINE>    ENDIF.</FPCLINE>
      <FPCLINE/>
      <FPCLINE>    SELECT mdocm</FPCLINE>
      <FPCLINE>           point</FPCLINE>
      <FPCLINE>           idate</FPCLINE>
      <FPCLINE>           itime</FPCLINE>
      <FPCLINE>           recdv</FPCLINE>
      <FPCLINE>           vlcod</FPCLINE>
      <FPCLINE>           FROM imrg</FPCLINE>
      <FPCLINE>           INTO TABLE lt_imrg</FPCLINE>
      <FPCLINE>           FOR ALL ENTRIES IN gt_imptt</FPCLINE>
      <FPCLINE>           WHERE point EQ gt_imptt-point.</FPCLINE>
      <FPCLINE/>
      <FPCLINE>    IF sy-subrc EQ 0.</FPCLINE>
      <FPCLINE>      SORT lt_imrg BY point ASCENDING idate DESCENDING itime DESCENDING.</FPCLINE>
      <FPCLINE>    ENDIF.</FPCLINE>
      <FPCLINE/>
      <FPCLINE>    LOOP AT gt_imptt ASSIGNING &lt;fs_imptt&gt;.</FPCLINE>
      <FPCLINE/>
      <FPCLINE>      lv_num = &lt;fs_imptt&gt;-desir_n.</FPCLINE>
      <FPCLINE>      WRITE lv_num TO &lt;fs_imptt&gt;-desir DECIMALS 2.</FPCLINE>
      <FPCLINE>      CONDENSE &lt;fs_imptt&gt;-desir.</FPCLINE>
      <FPCLINE/>
      <FPCLINE>      lv_num = &lt;fs_imptt&gt;-mrmin_n.</FPCLINE>
      <FPCLINE>      WRITE lv_num TO &lt;fs_imptt&gt;-mrmin DECIMALS 2.</FPCLINE>
      <FPCLINE>      CONDENSE &lt;fs_imptt&gt;-mrmin.</FPCLINE>
      <FPCLINE/>
      <FPCLINE>      lv_num = &lt;fs_imptt&gt;-mrmax_n.</FPCLINE>
      <FPCLINE>      WRITE lv_num TO &lt;fs_imptt&gt;-mrmax DECIMALS 2.</FPCLINE>
      <FPCLINE>      CONDENSE &lt;fs_imptt&gt;-mrmax.</FPCLINE>
      <FPCLINE/>
      <FPCLINE>      READ TABLE lt_imrg ASSIGNING &lt;fs_imrg&gt; WITH KEY point = &lt;fs_imptt&gt;-point</FPCLINE>
      <FPCLINE>                                             BINARY SEARCH.</FPCLINE>
      <FPCLINE>      IF sy-subrc EQ 0.</FPCLINE>
      <FPCLINE>        IF &lt;fs_imrg&gt;-vlcod IS NOT INITIAL.</FPCLINE>
      <FPCLINE>          &lt;fs_imptt&gt;-lsmrd = &lt;fs_imrg&gt;-vlcod.</FPCLINE>
      <FPCLINE>        ELSE.</FPCLINE>
      <FPCLINE>          lv_num = &lt;fs_imrg&gt;-recdv.</FPCLINE>
      <FPCLINE>          WRITE lv_num TO &lt;fs_imptt&gt;-lsmrd DECIMALS 2.</FPCLINE>
      <FPCLINE>          CONDENSE &lt;fs_imptt&gt;-lsmrd.</FPCLINE>
      <FPCLINE>        ENDIF.</FPCLINE>
      <FPCLINE>      ENDIF.</FPCLINE>
      <FPCLINE/>
      <FPCLINE>      READ TABLE lt_cabn ASSIGNING &lt;fs_cabn&gt; WITH KEY atinn = &lt;fs_imptt&gt;-atinn</FPCLINE>
      <FPCLINE>                                             BINARY SEARCH.</FPCLINE>
      <FPCLINE>      IF sy-subrc EQ 0.</FPCLINE>
      <FPCLINE>        &lt;fs_imptt&gt;-msehi = &lt;fs_cabn&gt;-msehi.</FPCLINE>
      <FPCLINE>      ENDIF.</FPCLINE>
      <FPCLINE/>
      <FPCLINE>      IF &lt;fs_imptt&gt;-codgr IS NOT INITIAL.</FPCLINE>
      <FPCLINE>        LOOP AT lt_qpct ASSIGNING &lt;fs_qpct&gt; WHERE katalogart = &lt;fs_imptt&gt;-codct AND</FPCLINE>
      <FPCLINE>                                                  codegruppe = &lt;fs_imptt&gt;-codgr.</FPCLINE>
      <FPCLINE>          ls_valcod-point = &lt;fs_imptt&gt;-point.</FPCLINE>
      <FPCLINE>          ls_valcod-codct = &lt;fs_imptt&gt;-codct.</FPCLINE>
      <FPCLINE>          ls_valcod-codgr = &lt;fs_imptt&gt;-codgr.</FPCLINE>
      <FPCLINE>          CONCATENATE &lt;fs_qpct&gt;-code &apos;-&apos; &lt;fs_qpct&gt;-kurztext</FPCLINE>
      <FPCLINE>          INTO ls_valcod-code SEPARATED BY space.</FPCLINE>
      <FPCLINE>          APPEND ls_valcod TO gt_valcodes.</FPCLINE>
      <FPCLINE>          CLEAR ls_valcod.</FPCLINE>
      <FPCLINE>        ENDLOOP.</FPCLINE>
      <FPCLINE>      ENDIF.</FPCLINE>
      <FPCLINE>    ENDLOOP.</FPCLINE>
      <FPCLINE>  ENDIF.</FPCLINE>
      <FPCLINE>ENDIF.</FPCLINE>
      <FPCLINE/>
      <FPCLINE>CALL METHOD cl_ssf_xsf_utilities=&gt;get_bds_graphic_as_bmp</FPCLINE>
      <FPCLINE>  EXPORTING</FPCLINE>
      <FPCLINE>    p_object       = &apos;GRAPHICS&apos;</FPCLINE>
      <FPCLINE>    p_name         = &apos;ZUGLOGO&apos;</FPCLINE>
      <FPCLINE>    p_id           = &apos;BMAP&apos;</FPCLINE>
      <FPCLINE>    p_btype        = &apos;BCOL&apos;</FPCLINE>
      <FPCLINE>  RECEIVING</FPCLINE>
      <FPCLINE>    p_bmp          = xstring</FPCLINE>
      <FPCLINE>  EXCEPTIONS</FPCLINE>
      <FPCLINE>    not_found      = 1</FPCLINE>
      <FPCLINE>    internal_error = 2</FPCLINE>
      <FPCLINE>    OTHERS         = 3.</FPCLINE>
      <FPCLINE/>
      <FPCLINE>SELECT SINGLE name_text</FPCLINE>
      <FPCLINE>       FROM user_addrp</FPCLINE>
      <FPCLINE>       INTO printby_desc</FPCLINE>
      <FPCLINE>       WHERE bname = sy-uname.</FPCLINE>
      <FPCLINE/>
      <FPCLINE>IF order_header-tplnr IS NOT INITIAL.</FPCLINE>
      <FPCLINE/>
      <FPCLINE>  CALL METHOD zcl_shoppaper_initialize=&gt;get_zone</FPCLINE>
      <FPCLINE>    EXPORTING</FPCLINE>
      <FPCLINE>      im_tplnr = order_header-tplnr</FPCLINE>
      <FPCLINE>    IMPORTING</FPCLINE>
      <FPCLINE>      ex_zonet = zone_desc.</FPCLINE>
      <FPCLINE/>
      <FPCLINE>ENDIF.</FPCLINE>
      <FPCLINE/>
      <FPCLINE>IF order_header-stort IS NOT INITIAL.</FPCLINE>
      <FPCLINE>  SELECT SINGLE ktext FROM t499s</FPCLINE>
      <FPCLINE>         INTO loc_desc</FPCLINE>
      <FPCLINE>         WHERE werks = order_header-swerk AND</FPCLINE>
      <FPCLINE>               stand = order_header-stort.</FPCLINE>
      <FPCLINE>ENDIF.</FPCLINE>
      <FPCLINE/>
      <FPCLINE>READ TABLE prt_order WITH KEY fhmar = &apos;P&apos; TRANSPORTING NO FIELDS.</FPCLINE>
      <FPCLINE>IF sy-subrc NE 0.</FPCLINE>
      <FPCLINE>  message = &apos;NO MEASUREMENT DOCUMENT PRTs LISTED&apos;.</FPCLINE>
      <FPCLINE>ENDIF.</FPCLINE>
     </INITIALIZATION>
